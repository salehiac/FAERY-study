{
    "nb_samples": 5000,
    "perplexities": [25, 75],

    "load_directory": "./data/solvers/assembly-v2",
    "save_path": "data/Images/solvers/assembly-v2", 
    "save_basename":"TSNE_{}",

    "is_sequential":false,

    "to_highlight": {
        "NS_-1":"dodgerblue",
