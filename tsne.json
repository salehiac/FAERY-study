{
    "env": "FAERY_buttonpress/FAERY_buttonpress_0",
    "nb_samples": 350000,
    "perplexities": [50],
    "is_sequential":false,
    
    "load_directory": "./results/{}",
    "save_path": "results/Images/nostop/buttonpress/TSNE/{}", 
    "save_basename":"TSNE_{}",

    "param_to_solver":false,
    "param_or_solver":"params",

    "pca_components": 500,
    "titles_side":["Parameter space {} perplexity and PCA of {} components", "Behavioral space {} perplexity"],

    "bland":false,

    "highlight":false,
    "to_highlight": {
        "buttonpress_0":"dodgerblue",
        "buttonpress_40":"red",
        "buttonpress_100":"green",
        "buttonpress_200":"gold",
        "buttonpress_400":"mediumorchid"
    },

    "follow":true,
    "to_follow": {
        "buttonpress_0":"dodgerblue",
        "buttonpress_40":"red",
        "buttonpress_100":"green",
        "buttonpress_200":"gold",
        "buttonpress_400":"mediumorchid"
    },
    "meta_steps":"all",

    "clustering":false,
    "cluster_quantile": 0.05,

    "params_plot": {
        "box_size": 8,
        "marker": "o",
        "base_color": "lightblue"
    },

    "params_anim": {
        "background_alpha":1,
        "inner_alpha":0.25,
        "types_run": ["train"],
        "animate_inner": true,
        "fps": 14,
        "dpi": 100,
        "time_pause": 1.5
    },

    "params_anim_to_solver": {
        "background_alpha":1,
        "animate_inner": false,
        "fps": 14,
        "dpi": 100,
        "time_pause": 1.5
    }
}